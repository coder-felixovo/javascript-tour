{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "no-var": "error",
        "no-sparse-arrays": "off",
        "no-compare-neg-zero": "warn",
        "no-unused-vars": "error"
    }
}
